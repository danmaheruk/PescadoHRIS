<!DOCTYPE html>
<% provide(:title, 'Admins') %>
<script>
$(document).ready(function(){$("body").hide().fadeIn(2000);});
</script>

<h1>System Administrators</h1>


<div class="row">
  <div clas="col-md-7 col-offset-1.5">
    <ul class="users">
      <li>
            <% @users.each do |user| %><p>
            <%= gravatar_for user, size: 50 %>
            <%= link_to user.name, user %>
          </li>
            <% end %>
</ul>
</div>
</div>
